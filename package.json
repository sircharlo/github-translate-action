{
  "author": "sircharlo",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/webhooks": "14.2.0",
    "@tomsun28/google-translate-api": "1.1.2",
    "franc-min": "6.2.0"
  },
  "description": "The action for translating Non-English issues content to English.",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "translate",
    "issues",
    "localization"
  ],
  "license": "MIT",
  "main": "lib/main.js",
  "name": "issues-translator",
  "overrides": {
    "@octokit/plugin-rest-endpoint-methods": "4.2.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sircharlo/github-translate-action.git"
  },
  "scripts": {
    "all": "npm run build && npm run format && npm run package",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "version": "1.0.2"
}
